[docker-ee-stable-17.03]
name=Docker EE Stable 17.03 - $basearch
baseurl=$dockerurl/$dockerosversion/$basearch/stable-17.03/
enabled=1
gpgcheck=1
gpgkey=$dockerurl/gpg

